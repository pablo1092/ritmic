		/**
		 * greatest common divisor (euclides)
		 *
		 * @return
		 */
		private int gcd(int a, int b) {
			// ArithmeticUtils.gcd(dur, mcdTupleElementDuration);
			if (b == 0) {
				return a;
			} else {
				return gcd(b, a % b);
			}
		}

		private int mcm(int a, int b) {
			int m = gcd(a, b);
			return (m * (a / m) * (b / m));
		}

