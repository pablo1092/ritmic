package es.ua.dlsi.im3.core.played;

public class Tempo {
	int tempo;
	long time;
	public Tempo(int value) {
		super();
		this.tempo = value;
	}
	public final int getTempo() {
		return tempo;
	}
	public final void setTempo(int value) {
		this.tempo = value;
	}
	public final long getTime() {
		return time;
	}
	public final void setTime(long time) {
		this.time = time;
	}
	
}
